{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm install && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/javascript; charset=utf-8"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "^/.*\\.js$",
      "headers": {
        "content-type": "application/javascript; charset=utf-8"
      },
      "continue": true
    },
    {
      "src": "^/.*\\.(css|json|map)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": ".*",
      "dest": "/index.html"
      
    }
  ]
} 